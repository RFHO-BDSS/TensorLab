{
  "graph":"vae",
  "input_dim":32,
  "output_dim":32,
  "use_regularization":1,
  "placeholders":{
    "source_tensor":{
      "name":"source-tensor",
      "shape":[null, 32, 32, 32],
      "type":"tf.float32"
    },
    "target_tensor":{
      "name":"target-tensor",
      "shape":[null, 32, 32, 32],
      "type":"tf.float32"
    },
    "is_training":{
      "default":{
        "value":0,
        "name":"is-training-default"
      },
      "name":"is-training",
      "type":"tf.bool",
      "shape":[]
    },
    "is_validating":{
      "default":{
        "value":0,
        "name":"is-validating-default"
      },
      "name":"is-validating",
      "type":"tf.bool",
      "shape":[]
    },
    "is_testing":{
      "default":{
        "value":0,
        "name":"is-testing-default"
      },
      "name":"is-testing",
      "type":"tf.bool",
      "shape":[]
    },
    "regularizer_scale":{
      "default":{
        "value":0.01,
        "name":"reg-scale-default"
      },
      "name":"regularizer-scale",
      "type":"tf.float32",
      "shape":[]
    }
  },
  "modules":{
    "encoder":{
      "name":"encoder",
      "size":1024
    },
    "latents":{
      "name":"latents",
      "size":256
    },
    "priors":{
      "name":"priors",
      "size":24
    },
    "decoder":{
      "name":"decoder"
    },
    "predictor":{
      "name":"predictor"
    },
    "informer":{
      "name":"informer",
      "size":128
    }
  },
  "parameters":{
    "step":{
      "value":0,
      "name":"step"
    },
    "loss":{
      "gamma":{
        "value":0.96,
        "name":"loss_gamma"
      },
      "offset":{
        "value":1e-7,
        "name":"loss_offset"
      }
    },
    "binary_threshold":{
      "min":0.4,
      "max":0.6
    },
    "dropout":{
      "use":0,
      "keep_prob":0.6
    }
  }
}